{
  "name": "scholargy-mvp-azure",
  "version": "1.0.0",
  "description": "Scholargy MVP with Azure integration",
  "private": true,
  "workspaces": [
    "frontend",
    "backend"
  ],
  "scripts": {
    "start": "node startup.js",
    "start:dev": "concurrently \"npm run start:backend\" \"npm run start:frontend\"",
    "start:backend": "cd backend && npm start",
    "start:frontend": "cd frontend && npm start",
    "build": "cd frontend && npm run build",
    "install:all": "npm install && cd frontend && npm install && cd ../backend && npm install",
    "test": "echo \"No tests specified\" && exit 0"
  },
  "dependencies": {
    "@azure/search-documents": "^12.0.0",
    "@azure/storage-blob": "^12.17.0",
    "axios": "^1.7.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "mongodb": "^6.3.0",
    "openai": "^4.20.1",
    "path": "^0.12.7",
    "redis": "^4.6.13",
    "studentvue": "latest"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "engines": {
    "node": ">=22.0.0",
    "npm": ">=10.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/scholargyMVPAzure.git"
  },
  "keywords": [
    "scholarship",
    "college",
    "education",
    "azure",
    "ai"
  ],
  "author": "Scholargy Team",
  "license": "MIT"
}
